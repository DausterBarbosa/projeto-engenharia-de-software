// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  nome String
  sobrenome String
  email String
  senha String
  trainings Training[]
  @@unique([email])
}

model Training {
  id Int @id @default(autoincrement())
  nome String
  repeticoes Int
  secoes Int
  carga Int
  diadasemana Int
  user User @relation(fields:[userId], references:[id])
  userId Int
}
